<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.parapf.eventsync.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_events"
        android:name="com.parapf.eventsync.ui.events.EventsFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_events" />

    <fragment
        android:id="@+id/nav_registered_events"
        android:name="com.parapf.eventsync.ui.RegisteredEvents.RegisteredEventsFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_registered_events" />


    <action
        android:id="@+id/action_events_to_registered"
        app:destination="@id/nav_registered_events" />


</navigation>